{
  "name": "content-management-agents-ts",
  "version": "1.0.0",
  "description": "Multi-agent content management system using LangChain.js and LangGraph.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:lambda": "tsc && cd dist && zip -r ../lambda.zip .",
    "build:cdk": "pwsh -File build-lambda-cdk.ps1",
    "start": "node dist/server.js",
    "dev": "tsx src/index.ts",
    "demo": "tsx src/index.ts --demo",
    "demo-html": "tsx src/demoHTMLPublisher.ts",
    "langgraph": "tsx src/workflows/langgraphWorkflow.ts",
    "enhanced": "tsx src/workflows/enhancedWorkflow.ts",
    "html": "tsx src/convertToHTML.ts",
    "clean": "rimraf dist",
    "server": "tsx src/server.ts",
    "dev:ui": "tsx src/server.ts",
    "deploy:serverless": "sam build && sam deploy --guided",
    "cdk": "cdk",
    "cdk:deploy": "pwsh -File build-lambda-cdk.ps1 && cdk deploy",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:destroy": "cdk destroy"
  },
  "keywords": [
    "langchain",
    "langgraph",
    "ai-agents",
    "content-management",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@langchain/core": "^0.2.0",
    "@langchain/langgraph": "^0.0.34",
    "@langchain/openai": "^0.2.0",
    "@types/aws-lambda": "^8.10.145",
    "aws-cdk": "^2.168.0",
    "aws-cdk-lib": "^2.168.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.4.1",
    "langchain": "^0.2.0",
    "source-map-support": "^0.5.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
